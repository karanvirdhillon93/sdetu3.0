package content;

public class Days {
	public static void main(String[] args) {
		//Execute different block of code based on the value of cond
		
		String dayOfWeek;
		dayOfWeek="Tuesday";
		switch (dayOfWeek) {
		case "Monday": System.out.print("Monday");	
		case "Tuesday": System.out.print("Tuesday");
		case "Wed":System.out.print("Wed");
		case "Thu":System.out.print("Thu");
		}
	}
}
